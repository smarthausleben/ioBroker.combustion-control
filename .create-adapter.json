{
	"expert": "yes",
	"cli": false,
	"adapterName": "combustion-control",
	"title": "Combustion Control",
	"description": "Adapter for Ocontrol combustion controller",
	"keywords": [
		"combustion",
		"ocontrol",
		"stove",
		"tile stove"
	],
	"authorName": "smarthausleben",
	"authorGithub": "smarthausleben",
	"authorEmail": "info@smarthausleben.de",
	"icon": {
		"data": "data:image/png;base64,",
		"extension": "png"
	},
	"features": [
		"adapter"
	],
	"adminFeatures": [
		"tab",
		"custom"
	],
	"type": "climate-control",
	"startMode": "daemon",
	"connectionType": "local",
	"dataSource": "push",
	"connectionIndicator": "yes",
	"adapterSettings": [
		{
			"key": "macAddress",
			"label": "Bluetooth device MAC address (if empty, first found 'oControl' device will be used.)",
			"inputType": "text",
			"defaultValue": ""
		},
		{
			"key": "deviceName",
			"label": "Bluetooth device name (if empty, default name 'oControl' will be used.))",
			"inputType": "text",
			"defaultValue": "oControl"
		},
		{
			"key": "updateIntervall",
			"label": "Update interval in seconds (minimu is 5 seconds. Default is 60 seconds.)",
			"inputType": "number",
			"defaultValue": "60"
		},
		{
			"key": "additionalMessages",
			"label": "More logging messages",
			"inputType": "checkbox",
			"defaultValue": false
		}
	],
	"language": "JavaScript",
	"nodeVersion": "14",
	"adminUi": "json",
	"tabReact": "no",
	"releaseScript": "yes",
	"indentation": "Tab",
	"quotes": "single",
	"es6class": "yes",
	"tools": [
		"ESLint",
		"type checking"
	],
	"gitRemoteProtocol": "HTTPS",
